type Recipe {
    id: ID!
    title: String!
    url: String
    imageUrl: String
    site: String
    tags: [Tag]
}

type Query {
    recipe(id: ID): Recipe
    recipes: [Recipe]!
}

type Mutation {
    newRecipe(title: String, url: String, imageUrl: String, site: String, recipeListId: ID): Recipe
}
