type RecipeList {
    id: ID!
    name: String!
    created: String!
    recipes: [Recipe]
}

extend type Query {
    recipeList(id: ID): RecipeList
    recipeLists: [RecipeList]!
}

input RecipeListInput {
    name: String!
    creatorId: ID!
}

extend type Mutation {
    newRecipeList(input: RecipeListInput): RecipeList
}